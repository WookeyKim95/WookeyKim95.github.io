    return

def arrive_ment_eng(id_num):
    
    if id_num['door'] == 'ì™¼ìª½':
        print('The door opens on the left.')
    elif id_num['door'] == 'ì˜¤ë¥¸ìª½':
        print('The door opens on the right.')
    print()
    if len(id_num['í™˜ìŠ¹']) &gt; 0:
        print('You can transfer to ', end=' ')
        for i in id_num['í™˜ìŠ¹']:
            if type(i) == int:
                print('line no.' + str(i), end=' ')
            elif type(i) == str:
                print(i+' line', end=' ')
        print('\n')
        
    return

def arrival(id_num):
    
    print('ì´ë²ˆì—­ì€ ' + id_num['name'] + ', ' + id_num['name'] + 'ì—­ ì…ë‹ˆë‹¤.')
    
    arrive_ment_kor(id_num)
    arrive_ment_eng(id_num)

    print('----------------------------')
    print()

    return

def arrival_term(id_num):
    
    print('ì´ë²ˆì—­ì€ ìš°ë¦¬ ì—´ì°¨ì˜ ë§ˆì§€ë§‰ ì—­ì¸ ' + id_num['name'] + ', ' + id_num['name'] + 'ì—­ ì…ë‹ˆë‹¤.')
    print()
    
    arrive_ment_kor(id_num)
        
    print('ë‚´ë¦¬ì‹¤ ë•Œì— ë‘ê³  ë‚´ë¦¬ì‹œëŠ” ë¬¼ê±´ì´ ì—†ëŠ”ì§€ ë‹¤ì‹œ í•œë²ˆ í™•ì¸í•˜ì‹œê¸° ë°”ëë‹ˆë‹¤.')
    print('ì˜¤ëŠ˜ë„ ì´ìš©í•´ì£¼ì…”ì„œ ê°ì‚¬í•©ë‹ˆë‹¤. ì•ˆë…•íˆ ê°€ì‹­ì‹œì˜¤!')
    print()
    
    print('This stop is '+ id_num['name'] + ', ' + id_num['name'] + ', The last station.')
    arrive_ment_eng(id_num)
    
    print('Thank you for using our train!')
    print('Good Bye!')
    print()
    print('----------------------------')
    print()

    return
